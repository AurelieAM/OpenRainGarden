# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /register                   controllers.Application.registerRainGarden()
GET     /browse-garden              controllers.Application.browseGarden()
GET     /register1                   controllers.Application.postRainGardenRegister()
GET     /upload                     controllers.Application.uploadRainGardenPicture()
POST    /upload                     controllers.Application.upload()
GET		/login						controllers.Application.login()
POST	/login						controllers.Application.postLogin()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
